{
  "name": "portal-auditorias-server",
  "version": "1.0.0",
  "description": "Portal de Auditorías Técnicas - Backend Node.js con IA local",
  "main": "server.js",
  "scripts": {
    "dev": "nodemon server.js",
    "start": "node server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "claude:generate": "node ../scripts/claude-md-generator.js",
    "claude:validate": "node ../scripts/claude-md-validator.js",
    "claude:rebuild": "npm run claude:generate:all && npm run claude:validate",
    "db:migrate": "npx sequelize-cli db:migrate",
    "db:seed": "npx sequelize-cli db:seed:all",
    "db:reset": "npx sequelize-cli db:drop && npx sequelize-cli db:create && npx sequelize-cli db:migrate && npx sequelize-cli db:seed:all"
  },
  "keywords": [
    "auditorias",
    "call-center",
    "nodejs",
    "express",
    "sequelize",
    "ollama",
    "ia-local",
    "etl",
    "mysql"
  ],
  "author": "Portal Auditorías Team",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.10.0",
    "bcryptjs": "^2.4.3",
    "bullmq": "^4.15.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "exceljs": "^4.4.0",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.2.1",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.6.5",
    "papaparse": "^5.4.1",
    "pdf-parse": "^1.1.1",
    "sequelize": "^6.35.1",
    "sharp": "^0.33.0",
    "validator": "^13.11.0",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.8",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "sequelize-cli": "^6.6.2",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tu-organizacion/portal-auditorias.git"
  },
  "bugs": {
    "url": "https://github.com/tu-organizacion/portal-auditorias/issues"
  },
  "homepage": "https://github.com/tu-organizacion/portal-auditorias#readme"
}
